{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": ["sku", "name", "stock"],
  "properties": {
    "sku": {
      "$id": "#/properties/sku",
      "type": "string",
      "title": "The Sku Schema",
      "default": "",
      "examples": ["SKU006"],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": ["Airpods 2"],
      "pattern": "^(.*)$"
    },
    "stock": {
      "$id": "#/properties/stock",
      "type": "integer",
      "title": "The Stock Schema",
      "default": 0,
      "examples": [10]
    }
  }
}
